{
    "extends": "./node_modules/@microsoft/rush-stack-compiler-3.9/includes/tsconfig-web.json",
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "strict": true,
        "noImplicitThis": false,
        "allowJs": true,
        "declaration": false,
        "declarationMap": false,
        "noUnusedLocals": false,
        "noImplicitReturns": true,
        "noUnusedParameters": false,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "strictNullChecks": true,
        "alwaysStrict": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "target": "es5",
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "jsx": "react",
        "sourceMap": true,
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "outDir": "lib",
        "paths": {
            "@app/*": [
                "./src/app/*",
            ],
            "@components": [
                "./src/app/WelcomePoc2/components"
            ]
        },
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules/@microsoft",
            "./node_modules/@wm"
        ],
        "types": [
            "es6-promise",
            "webpack-env"
        ],
        "lib": [
            "es5",
            "es2015",
            "dom",
            "dom.iterable",
            "es2015.collection"
        ]
    },
    "exclude": [
        "node_modules",
        "lib"
    ],
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
    ]
}